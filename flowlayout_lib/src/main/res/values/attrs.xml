<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FlowLayout">
        <attr name="max_select" format="integer|enum">
            <!--单选-->
            <enum name="SINGLECHOICE" value="1"/>
            <!--多选-->
            <enum name="MULTIPLECHOICE" value="0"/>
        </attr>
        <attr name="flow_gravity" format="enum">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="isCheckedEnable" format="boolean"/>
    </declare-styleable>
</resources>